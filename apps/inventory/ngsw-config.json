{
  "index": "/index.html",
  "navigationUrls": [
    "/**"
  ],
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(png|jpg|jpeg|svg|gif)"
        ]
      }
    }
  ],
  "navigationFallback": {
    "index": "/index.html",
    "rewrite": true
  }
}